{
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true,
    "browser": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-console": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "2017",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
